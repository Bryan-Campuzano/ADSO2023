Algoritmo PromedioFijo
	Escribir "Escriba las 16 notas que quiere promediar"
	leer c1;
	leer c2;
	leer c3;
	leer c4;
	leer c5;
	leer c6;
	leer c7;
	leer c8;
	leer c9;
	leer c10;
	leer c11;
	leer c12;
	leer c13;
	leer c14;
	leer c15;
	leer c16;
	suma <- (c1+c2+c3+c4+c5+c6+c7+c8+c9+c10+c11+c12+c13+c14+c15+c16)
	promedio = suma/16
	Escribir " Tu promedio es :", promedio
FinAlgoritmo
